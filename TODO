http://ase.tufts.edu/gsc/gradresources/guidetomixedmodelsinr/mixed%20model%20guide.html

http://www.stat.wisc.edu/courses/st572-larget/Spring2007/handouts16-4.pdf

http://www.maths.bath.ac.uk/~jjf23/mixchange/split.html

require(car)
require(MASS)

qqp(AUDPS$LShB_rating, "norm")

qqp(AUDPS$LShB_rating, "lnorm")

poisson <- fitdistr(AUDPS$LShB_rating, "Poisson")
qqp(AUDPS$LShB_rating, "pois", poisson$estimate)

gamma <- fitdistr(AUDPS$LShB_rating, "gamma")
qqp(AUDPS$LShB_rating, "gamma", shape = gamma$estimate[[1]], rate = gamma$estimate[[2]])


LShB_lmm <- lmer(LShB_rating ~ WMGT * NRTE + (1 | REP/WMGT) + (1 | YEAR), data = AUDPS,
REML = FALSE)
summary(lmm)

TShB_lmm <- lmer(TShB_rating ~ WMGT * NRTE + (1 | REP/WMGT) + (1 | YEAR), data = AUDPS,
REML = FALSE)
summary(lmm)
